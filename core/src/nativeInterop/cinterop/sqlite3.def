package = sqlite3
headers = sqlite3.h
headerFilter = sqlite3*.h

linkerOpts = -lpthread -ldl

noStringConversion = sqlite3_prepare_v2 sqlite3_prepare_v3
---
struct ksqlite_vtab {
  struct sqlite3_vtab vtab;
  void* userObj;
};

struct ksqlite_vtab_cursor {
  struct sqlite3_vtab_cursor vtab_cursor;
  void* userObj;
};
